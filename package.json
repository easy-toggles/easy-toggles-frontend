{
  "name": "client",
  "version": "1.0.0",
  "description": "Front end for easy toggles",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --inline",
    "start:stub": "npm run stubby | npm start",
    "build": "webpack --mode production",
    "test": "jest",
    "test:unit": "jest --config jest.config.unit.js",
    "test:unit:ci": "npm run test:unit -- --runInBand",
    "test:integration": "jest --config jest.config.integration.js",
    "test:integration:ci": "npm run test:integration -- --runInBand",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "format": "prettier-stylelint --write './src/**/*' './test/**/*' --quiet",
    "stubby": "stubby --watch --quiet --data stubby.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.10.2",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.8.2",
    "@types/react-router-dom": "^4.3.4",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "1.18.2",
    "prettier-stylelint": "^0.4.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "stubby": "^4.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-idiomatic-order": "^7.0.3",
    "stylelint-config-recommended": "^2.2.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "immer": "^8.0.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-collapse": "^4.0.3",
