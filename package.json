    "react-router-dom": "^5.0.1",
    "react-tabs": "^3.0.0",
    "redux": "^4.0.4",
    "redux-arc": "^0.7.5",
    "redux-form": "^8.2.4",
    "redux-saga": "^1.0.5",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run format",
      "git add"
    ]
  }
}
